<Window x:Class="sportMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sport"
        mc:Ignorable="d"
        Title="ИС по прокату спортивного инвентаря" Height="800" Width="1200"
        MinHeight="600" MinWidth="900"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF76E383">
            <Image Source="/Resources/logo.png" HorizontalAlignment="Left" Margin="10"/>
            <Label Content="ИС по прокату спортивного инвентаря" 
                   HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="26">
            </Label>
            <Frame x:Name="MainFrame"
                   NavigationUIVisibility="Hidden">
                
            </Frame>

        </Grid>
    </Grid>
</Window>
